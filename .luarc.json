{
  "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
  "format": {
    "enable": true,
    "defaultConfig": {}
  },
  "completion": {
    "callSnippet": "Both"
  },
  "runtime": {
    "version": "LuaJIT",
    "builtin": {
      "basic": "enable",
      "builtin": "enable"
    }
  },
  "workspace": {
    "maxPreload": 100000,
    "preloadFileSize": 100000
  },
  "misc.parameters": ["--locale=en-us"],
    "workspace.checkThirdParty": false
}
